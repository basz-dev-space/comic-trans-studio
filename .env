# Environment variables for comic-trans-studio
# Copy this file to `.env` and configure before running the app.

# SQLite database (file-based, no external database server needed)
DATABASE_URL=file:./prisma/dev.db

# Runtime settings
NODE_ENV=development
PORT=5173

# Session TTL (milliseconds - default 24 hours)
ADMIN_SESSION_TTL_MS=86400000

# Admin management credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123
